{
  "name": "Page Load Time Tracker",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Tracks and displays how long pages take to load.",
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Page Load Time"
  }
}
